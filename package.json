{
    "name": "mapcss",
    "displayName": "mapcss",
    "description": "Language support for MapCSS files",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.61.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "mapcss",
            "aliases": ["MapCSS", "mapcss"],
            "extensions": [".mapcss"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "mapcss",
            "scopeName": "source.mapcss",
            "path": "./syntaxes/mapcss.tmLanguage.json"
        }]
    }
}